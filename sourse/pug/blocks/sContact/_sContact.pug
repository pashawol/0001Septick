mixin sContact()
	// start sContact
	+b.SECTION.sContact.section#sContact&attributes(attributes)
		.container
			.row
				.col-xl-8.order-xl-last
					+e.map
						<script type="text/javascript" charset="utf-8" async src="https://api-maps.yandex.ru/services/constructor/1.0/js/?um=constructor%3Aa8998517925a70899d00c82c8f8d90faf47cf2f38f50723d6df000d11efb6a33&amp;width=100%25&amp;height=650&amp;lang=ru_RU&amp;scroll=true"></script>
					.row.row-sm
						.col-sm-4
							+e.img-wrap(href="img/@2x/contacts.png" data-fancybox="contacts")
								img(src="img/@2x/contacts.png", alt="" )
						.col-sm-4
							+e.img-wrap(href="img/@2x/contacts.png" data-fancybox="contacts")
								img(src="img/@2x/contacts.png", alt="" )
						.col-sm-4
							+e.img-wrap(href="img/@2x/contacts.png" data-fancybox="contacts")
								img(src="img/@2x/contacts.png", alt="" )
						
				.col-xl-4
					+b.ASIDE.sContactAside
						+e.wrap
							.row
								.col-md-6.col-xl-12
									+e.item--withIcon
										+e.title Телефон и мессенджеры
											+icon('phone')
										+e.content
											+e.tel(href="tel:+74951156707") 8 (495) 115-67-07
											+b.soc
												+item-sos()
													+pic(`soc-5.png`,'',0)
												+item-sos()
													+pic(`soc-6.png`,'',0)
												+item-sos()
													+pic(`soc-7.png`,'',0)
									+e.item--withIcon
										+e.title Адрес офиса
											+icon('map-tag')
										+e.content
											span м. Окская, Рязанский пр-т, д.24, стр.2
									+e.item--withIcon
										+e.title Режим работы
											+icon('clock')
										+e.content.flex-column.align-items-start
											span ПН-ПТ с 9:00 до 21:00
											span СБ-ВС с 10:00 до 20:00
									+e.item--withIcon
										+e.title Email
											+icon('mail')
										+e.content
											+e.mail(href="mailto:info@septik.net") info@septik.net
									+e.item.mb-0
										+e.title Мы в социальных сетях
										+e.content
											+b.soc
												+item-sos()
													+pic(`soc-1.png`,'',0)
												+item-sos()
													+pic(`soc-2.png`,'',0)
												+item-sos()
													+pic(`soc-3.png`,'',0)
												+item-sos()
													+pic(`soc-4.png`,'',0)
									+e.item.d-md-none.d-xl-block
								.col-md-6.col-xl-12
									+e.item.mb-0
										+e.title Наши реквизиты
										+e.content
											ul
												li OOO &laquo;Мос-Септик&raquo;
												li #[b ИНН] 7721315867
												li #[b КПП] 772101001
												li #[b ОГРН] 1157746513231
											ul
												li #[b Расчетный счет] 40702810100150004030
												li #[b Название банка] АО&nbsp;&laquo;АЛЬФА-БАНК&raquo;
												li #[b БИК] 044525593
												li #[b Кор.счет] 30101810200000000593
											span  #[b Генеральный директор] Исякаев Тимур Марсович



	// end sContact